import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        String str=sc.next();
        int arr=sc.nextInt();
        int cpga=sc.nextInt();
        System.out.println("Name of the Student:"+str);
        if(arr==1&&cpga>70){
            System.out.printf("%s is Eligible for Placement",str);
        }
          else if((arr==2||arr==1)&&cpga>75){
               System.out.printf("%s is Eligible for Placement",str);
           }
           else {
            System.out.printf("%s is Not Eligible for Placement",str);
           }
       
        }
    
}

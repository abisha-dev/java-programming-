import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        String str1 = sc.nextLine();
         String str2=str1.replaceAll("[AEIOUaeiou]","");
        System.out.println(str2);
    }
}
        

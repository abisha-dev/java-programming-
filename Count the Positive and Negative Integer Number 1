import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc=new Scanner (System.in);
        int a=sc.nextInt();
        int arr[]=new int[a];
        float countpos=0;
        float countneg=0;
        for(int i=0;i<a;i++){
            arr[i]=sc.nextInt();
            
            if(arr[i]>0){
                
                countpos++;
            }else{
                countneg++;
                
            }
                 
                
            }
            System.out.printf("Count of Positive Integer is %.2f\n",countpos);
            System.out.printf("Count of Negative Integer is %.2f\n",countneg);
                
            
        }
    }
